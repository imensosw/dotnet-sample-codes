<body style="background: #F0F4F7;">
    <div>
        @RenderBody()
    </div>
    <script src="~/Content/data-tables/jquery.dataTables.min.js"></script>
    <script src="~/Content/data-tables/DT_bootstrap.js"></script>
    <link href="~/Content/data-tables/DT_bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Content/assets/css/style.css" rel="stylesheet" />

    <script>       
        $(document).ready(function () {
            $(".hide_show").click(function () {
                $(this).toggleClass("left_shift");
                var img = $(this).find("img");
                if ($(img).attr('src') === "~/images/menu.png") {
                    $(img).attr('src', "~/images/close.png");
                } else {
                    $(img).attr('src', "~/images/menu.png")
                }
                $(".left_panel, .right_panel").toggleClass("collaps");
            });
        });
    </script>
</body>

